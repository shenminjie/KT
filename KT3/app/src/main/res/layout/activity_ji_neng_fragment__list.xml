<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ji_neng_fragment__list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.newer.kt.fragment.peixun.JiNengFragment_List">
    <RelativeLayout
        android:id="@+id/rl_vs_titile_jineng"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/text_black">

        <ImageView
            android:id="@+id/image_vs_item_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:src="@mipmap/nomal_back" />

        <TextView
            android:id="@+id/tv_title_game"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="控球类"
            android:textColor="@color/gold"
            android:textSize="18dp" />


    </RelativeLayout>

    <ListView
        android:layout_below="@+id/rl_vs_titile_jineng"
        android:id="@+id/lv_jinengList"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>

</RelativeLayout>
